{
    "name": "greelogix/myfatoorah-laravel",
    "description": "Laravel package for MyFatoorah payment gateway integration with admin settings, webhooks, and recurring payments.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "myfatoorah",
        "payment",
        "gateway",
        "payment-processing",
        "recurring-payments",
        "webhook",
        "kuwait",
        "middle-east"
    ],
    "homepage": "https://github.com/greelogix/myfatoorah",
    "support": {
        "email": "asad.ali@greelogix.com",
        "issues": "https://github.com/greelogix/myfatoorah/issues",
        "source": "https://github.com/greelogix/myfatoorah"
    },
    "authors": [
        {
            "name": "Asad ALi",
            "email": "asad.ali@greelogix.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/view": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Greelogix\\MyFatoorah\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greelogix\\MyFatoorah\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Greelogix\\MyFatoorah\\MyFatoorahServiceProvider"
            ],
            "aliases": {
                "MyFatoorah": "Greelogix\\MyFatoorah\\Facades\\MyFatoorah"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}